<odoo>
    <data>
        <record id="office365_cron_do_calendar_import" model="ir.cron">
            <field name="name">Auto Import Office365 Contacts </field>
            <field name="model_id" ref="model_res_users"/>
            <field name="state">code</field>
            <field name="active">0</field>
            <field name="code">model.auto_import_contact()</field>
            <field name='interval_number'>3</field>
            <field name="user_id" ref="base.user_root"/>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
        </record>
        <record id="office365_cron_do_calendar_export" model="ir.cron">
            <field name="name">Auto Export Contacts</field>
            <field name="model_id" ref="model_res_users"/>
            <field name="state">code</field>
            <field name="active">0</field>
            <field name="code">model.auto_export_contact()</field>
            <field name='interval_number'>4</field>
            <field name="user_id" ref="base.user_root"/>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
        </record>



    </data>
</odoo>